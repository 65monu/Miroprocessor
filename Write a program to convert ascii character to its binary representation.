.model small
.data
msg db 10,13,"Enter an ASCII Character:: $"
result db 10,13,"Result is:: $"
.code
.startup
MOV DX,OFFSET msg
MOV AH,09H
INT 21H
MOV AH,01H
INT 21H
MOV BL,AL
MOV DX,OFFSET result
MOV AH,09H
INT 21H
MOV CL,08H
MOV AH,00H
MOV AL,BL
L1:SHL AL,01H
MOV BL,AL
MOV AL,00H
ADC AL,30H
MOV DL,AL
MOV AH,02H
INT 21H
MOV AL,BL
LOOP L1
.EXIT
END
